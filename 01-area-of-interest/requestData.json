{
  "layout": "A4 portrait",
  "outputFormat": "png",
  "attributes": {
    "map": {
	"longitudeFirst": true,
	"areaOfInterest": {
            "area": {
		"type": "MultiPoint",
		"coordinates": [
		    [
			0,
			40
		    ],
		    [
			20,
			60
		    ]
		]
            }
	},
      "projection": "EPSG:4326",
      "dpi": 72,
      "rotation": 0,
      "layers": [
        {
          "type": "geojson",
          "geoJson": "file://european-union-countries.geojson",
          "style": {
            "version": "2",
            "*": {
              "symbolizers": [
                {
                  "type": "polygon",
                  "fillColor": "#5E7F99",
                  "fillOpacity": 1,
                  "strokeColor": "#CC1D18",
                  "strokeOpacity": 1,
                  "strokeWidth": 1
                }
              ]
            }
          }
        }
      ]
    }
  }
}
